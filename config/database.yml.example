defaults: &defaults
  adapter: sqlite3
  database: db/test.sqlite3
  pool: 5
  timeout: 5000

development:
  <<: *defaults
  database: db/development.sqlite3

test:
  <<: *defaults
  database: db/test.sqlite3<%= ENV['TEST_ENV_NUMBER'] %>

production:
  <<: *defaults
  database: db/production.sqlite3
